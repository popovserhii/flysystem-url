{
    "name": "mrubiosan/flysystem-url",
    "description": "Flysystem URL generation from Cloud providers like Aws S3 and Azure Blob",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Marc Rubio",
            "email": "marc.rubio@go1.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Mrubiosan\\FlyUrl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrubiosan\\FlyUrl\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=5.6",
        "league/flysystem": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "league/flysystem-aws-s3-v3": "^1.0",
        "squizlabs/php_codesniffer": "^3.4",
        "league/flysystem-azure-blob-storage": "^0.1.5",
        "superbalist/flysystem-google-storage": "^7.2"
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "Support for AWS S3",
        "league/flysystem-azure-blob-storage": "Support for Azure Blob Storage",
        "superbalist/flysystem-google-storage": "Support for Google Cloud Storage"
    },
    "keywords": ["flysystem", "url", "aws", "s3" ,"azure", "blob", "google", "cloud", "storage"]
}
